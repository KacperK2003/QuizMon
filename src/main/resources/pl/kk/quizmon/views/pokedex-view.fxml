<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.HBox?>
<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="720.0" prefWidth="1280.0"
      fx:id="root"
      styleClass="root"
      xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="pl.kk.quizmon.controllers.PokedexController"
      stylesheets="@../styles/base.css, @../styles/pokedex.css">
    <HBox fx:id="mainHBox" spacing="100">
        <Button text="★" maxWidth="30" maxHeight="30" onAction="#onFavouriteClick"/>
    </HBox>
    <Button maxWidth="-Infinity" mnemonicParsing="false" text="Powrót" onAction="#onBackButtonClick"/>
</VBox>

